container_commands:
  01_compile_typescript:
    command: "npm run build:simple"
  02_verify_compiled_file:
    command: "ls -la server/index.js && echo 'Arquivo JavaScript compilado com sucesso' || echo 'ERRO: Arquivo server/index.js não encontrado'"
  03_check_dependencies:
    command: "echo 'Verificando dependências...' && ls -la node_modules/typescript && echo 'TypeScript OK'" 